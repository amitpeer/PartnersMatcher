<UserControl x:Class="PartnersMatcher.AdSummary"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PartnersMatcher"
             mc:Ignorable="d" 
             d:DesignHeight="100" d:DesignWidth="300">

    <Canvas HorizontalAlignment="Left" Height="100" VerticalAlignment="Top" Width="300" Background="White">
    <Grid Height="100" Width="300">
        <Label x:Name="label" Content="Ad Summary" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="115,0,0,0" FontWeight="Bold" />
        <Button x:Name="button" Content="Open ad" HorizontalAlignment="Left" Height="27" Margin="199,63,0,0" VerticalAlignment="Top" Width="91" RenderTransformOrigin="-0.034,4.273" Click="button_Click"/>
        <TextBlock x:Name="adid" HorizontalAlignment="Left" Height="17" Margin="10,33,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="50"><Span Foreground="Blue" FontSize="12.6666666666667" FontFamily="Consolas" FontWeight="Bold"><Run Text="adid"/></Span></TextBlock>
        <TextBlock x:Name="adid1" HorizontalAlignment="Left" Height="17" Margin="97,34,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="50"><Span Foreground="Blue" FontSize="12.6666666666667" FontFamily="Consolas" FontWeight="Bold"><Run Text="adid"/></Span></TextBlock>

        <TextBlock x:Name="category" HorizontalAlignment="Left" Height="17" Margin="10,50,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="63"><Span Foreground="Blue" FontSize="12.6666666666667" FontWeight="Bold" FontFamily="Consolas"><Run Text="category"/></Span></TextBlock>
        <TextBlock x:Name="category1" HorizontalAlignment="Left" Height="17" Margin="97,51,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="63"><Span Foreground="Blue" FontSize="12.6666666666667" FontWeight="Bold" FontFamily="Consolas"><Run Text="category"/></Span></TextBlock>

        <TextBlock x:Name="Location" HorizontalAlignment="Left" Height="17" Margin="10,70,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="63"><Span Foreground="Blue" FontSize="12.6666666666667" FontWeight="Bold" FontFamily="Consolas"><Run Text="Location"/></Span></TextBlock>
        <TextBlock x:Name="Location1" HorizontalAlignment="Left" Height="17" Margin="97,68,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="63"><Span Foreground="Blue" FontSize="12.6666666666667" FontWeight="Bold" FontFamily="Consolas"><Run Text="Location"/></Span></TextBlock>


    </Grid>
    </Canvas>
</UserControl>
